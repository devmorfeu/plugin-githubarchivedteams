{
  "name": "Github Hide Archived",
  "description": "Hides archived projects from the teams tab within the organization",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "/images/icon-16x16.png",
    "32": "/images/icon-32x32.png",
    "48": "/images/icon-48x48.png",
    "128": "/images/icon-128x128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon-16x16.png",
      "32": "/images/icon-32x32.png",
      "48": "/images/icon-48x48.png",
      "128": "/images/icon-128x128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://github.com/orgs/*/teams/*"
      ],
      "js": [
        "github-service.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}