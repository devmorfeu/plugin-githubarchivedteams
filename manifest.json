{
    "manifest_version": 3,
    "name": "Github Hide Archived",
    "description": "Hides archived projects from the times tab within the organization",
    "version": "1.0",
    "content_scripts": [
      {
        "js": ["theMagic.js"],
        "css": ["style.css"],
        "matches": [
          "*://github.com/orgs/*/teams/*/repositories"
        ]
      }
    ]
  }